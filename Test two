local ScreenGui = Instance.new("ScreenGui")
local maingui = Instance.new("Frame")
local close = Instance.new("TextButton")
local txt = Instance.new("TextLabel")
local im = Instance.new("TextButton")
local mt = Instance.new("TextButton")
local ray = Instance.new("TextButton")
local ground = Instance.new("TextButton")
local lb = Instance.new("TextButton")
local ky = Instance.new("TextButton")
local vrson = Instance.new("TextLabel")
local opengui = Instance.new("Frame")
local open = Instance.new("TextButton")

ScreenGui.Parent = game.CoreGui

maingui.Name = "maingui"
maingui.Parent = ScreenGui
maingui.Active = true
maingui.BackgroundColor3 = Color3.new(1, 0.666667, 0)
maingui.BorderColor3 = Color3.new(1, 0.333333, 0)
maingui.BorderSizePixel = 5
maingui.Position = UDim2.new(0.268379748, 0, 0.353808373, 0)
maingui.Size = UDim2.new(0, 559, 0, 252)
maingui.Visible = false
maingui.Active = true
maingui.Draggable = true

close.Name = "close"
close.Parent = maingui
close.BackgroundColor3 = Color3.new(1, 0.666667, 0)
close.BorderColor3 = Color3.new(1, 0.333333, 0)
close.BorderSizePixel = 5
close.Position = UDim2.new(0.883720934, 0, 0, 0)
close.Size = UDim2.new(0, 65, 0, 30)
close.Font = Enum.Font.SourceSans
close.Text = "X"
close.TextColor3 = Color3.new(0, 0, 0)
close.TextScaled = true
close.TextSize = 14
close.TextWrapped = true
close.MouseButton1Down:connect(function()
maingui.Visible = false
opengui.Visible = true
end)

txt.Name = "txt"
txt.Parent = maingui
txt.BackgroundColor3 = Color3.new(1, 0.666667, 0)
txt.BorderColor3 = Color3.new(1, 0.333333, 0)
txt.BorderSizePixel = 5
txt.Size = UDim2.new(0, 494, 0, 30)
txt.Font = Enum.Font.SourceSans
txt.Text = "By:  The Exploiter"
txt.TextColor3 = Color3.new(0, 0, 0)
txt.TextScaled = true
txt.TextSize = 14
txt.TextWrapped = true

im.Name = "im"
im.Parent = maingui
im.BackgroundColor3 = Color3.new(1, 0.666667, 0)
im.BorderColor3 = Color3.new(1, 0.333333, 0)
im.BorderSizePixel = 5
im.Position = UDim2.new(0.050089445, 0, 0.257936507, 0)
im.Size = UDim2.new(0, 162, 0, 50)
im.Font = Enum.Font.SourceSans
im.Text = "Ice Mewtwo"
im.TextColor3 = Color3.new(0, 0, 0)
im.TextScaled = true
im.TextSize = 14
im.TextWrapped = true
im.MouseButton1Down:connect(function()
local pokemon = "Ice Mewtwo"
local lvl     = "100"

local Encrypt = function(CodeSource)
local StringCode = '';
for i = 1, #CodeSource do
StringCode = StringCode ..string.char(48)..string.byte(string.sub(CodeSource,i,i));
end
return StringCode;
end

local pass = Encrypt(game.Players.LocalPlayer.Name..lvl..pokemon)

game.ReplicatedStorage.REvents.Pokemon.iwillknowXD:InvokeServer(pokemon, game.Players.LocalPlayer.PokemonParty, lvl, true, true, pass)
end)

